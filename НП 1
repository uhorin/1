def is_happy_number(number):
    while number != 0:
        digit = number%10
        if digit != 4 and digit != 7:
            return False
        else:
            number = number//10 
    return True
            
def count_happy_number(N):
   count = 0
   for i in range(1, N):
       if is_happy_number(i):
           count += 1
           print(i)
   return count


N = int(input("Enter N: "))
if N<=0:
    print("Sorry, N is not correct")
result = count_happy_number(N)
print(f"The number of lucky numbers not greater than {N}: {result}")